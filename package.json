{
    "name": "project-cli",
    "version": "1.0.0",
    "description": "project-cli 脚手架工具 命令行交互工具",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src/index",
        "precommit": "pretty-quick --pattern 'src/**/*.*(js|ts|jsx)' --staged"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "pretty-quick --pattern='src/**/*.*(js|ts|jsx)' --staged"
        ]
    },
    "keywords": [
        "cli",
        "project"
    ],
    "author": "caowencheng <845982120@qq.com>",
    "license": "ISC",
    "dependencies": {
        "husky": "^4.2.5",
        "inquirer": "^7.1.0",
        "lint-staged": "^10.1.7",
        "ora": "^4.0.4",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "ts-node": "^8.9.0",
        "typescript": "^3.8.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/node": "^13.13.1",
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "eslint": "^6.8.0"
    }
}
